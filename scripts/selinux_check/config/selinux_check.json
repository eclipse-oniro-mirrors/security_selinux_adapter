{
    "selinux_check": [
        {
            "script": "base/security/selinux_adapter/scripts/selinux_check/check_data_regex.py",
            "args": "--file_contexts",
            "extra_args": "--whitelist whitelist/data_regex_whitelist.txt"
        },
        {
            "script": "base/security/selinux_adapter/scripts/selinux_check/check_partition_label_use.py",
            "args": "--file_contexts",
            "extra_args": "--whitelist whitelist/partition_label_use_whitelist.txt --config config/partition_label.txt"
        },
        {
            "script": "base/security/selinux_adapter/scripts/selinux_check/check_perm_group.py",
            "args": "--cil_file",
            "extra_args": "--whitelist whitelist/perm_group_whitelist.json --config config/perm_group.json"
        }
    ]
}