neverallow { system_domain -init -appspawn } { vendor_etc_file }:dir { read };
neverallow { system_domain -init -appspawn } { vendor_etc_file }:dir { open };

neverallow { system_domain } { vendor_etc_file }:file ~{ map open read getattr };
